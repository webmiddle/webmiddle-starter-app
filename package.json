{
  "name": "webmiddle-starter-app",
  "private": true,
  "version": "0.1.0",
  "description": "webmiddle starter app",
  "main": "dist/index.js",
  "files": [
    "dist"
  ],
  "engines": {
    "node": "^6.14.0 || >=8.10.0"
  },
  "scripts": {
    "test": "cross-env NODE_ENV=test ava",
    "test:watch": "npm run test -- --watch",
    "clean": "rimraf dist",
    "babel": "babel src --out-dir dist --source-maps inline",
    "build": "npm run clean && npm run babel",
    "build:watch": "npm run clean && npm run babel -- --watch",
    "start": "nodemon --watch src --exec \"npm run clean && npm run build && npm run start:server\"",
    "start:server": "node dist/server",
    "prepublish": "npm run clean && npm run build && npm run test"
  },
  "ava": {
    "files": [
      "src/tests/*.js",
      "!**/index.js"
    ],
    "source": [
      "**/*.{js,jsx}",
      "!dist/**/*"
    ],
    "concurrency": 5,
    "failFast": true,
    "tap": true,
    "require": [
      "babel-register"
    ],
    "babel": "inherit"
  },
  "dependencies": {
    "webmiddle": "^0.4.0",
    "webmiddle-component-arraymap": "^0.4.0",
    "webmiddle-component-cheerio-to-json": "^0.4.0",
    "webmiddle-component-cheerio-to-virtual": "^0.4.0",
    "webmiddle-component-http-request": "^0.4.0",
    "webmiddle-component-jsonselect-to-json": "^0.4.0",
    "webmiddle-component-jsonselect-to-virtual": "^0.4.0",
    "webmiddle-component-parallel": "^0.4.0",
    "webmiddle-component-pipe": "^0.4.0",
    "webmiddle-component-resume": "^0.4.0",
    "webmiddle-component-virtual-to-json": "^0.4.0",
    "webmiddle-server": "^0.4.0"
  },
  "devDependencies": {
    "ava": "^0.25.0",
    "babel-cli": "^6.24.1",
    "babel-core": "^6.24.1",
    "babel-eslint": "^7.2.2",
    "babel-plugin-transform-react-jsx": "^6.24.1",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-env": "^1.4.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "babel-register": "^6.24.1",
    "babel-runtime": "^6.23.0",
    "cross-env": "^5.2.0",
    "eslint": "^3.19.0",
    "eslint-config-airbnb": "^14.1.0",
    "eslint-config-prettier": "^3.1.0",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^4.0.0",
    "eslint-plugin-prettier": "^3.0.0",
    "eslint-plugin-react": "^6.10.3",
    "husky": "^1.1.2",
    "lint-staged": "^7.3.0",
    "nodemon": "^1.18.4",
    "prettier": "^1.14.3",
    "rimraf": "^2.6.1"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.js": [
      "prettier --write",
      "git add"
    ]
  }
}
