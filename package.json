{
  "name": "webmiddle-starter-app",
  "version": "0.1.0",
  "description": "webmiddle starter app",
  "author": "Manuel Dell'Elce",
  "license": "MIT",
  "main": "dist/index.js",
  "files": [
    "dist"
  ],
  "engines": {
    "node": "^6.14.0 || >=8.10.0"
  },
  "scripts": {
    "test": "cross-env NODE_ENV=test ava",
    "test:watch": "npm run test -- --watch",
    "clean": "rimraf dist",
    "babel": "babel src --out-dir dist --source-maps inline",
    "build": "npm run clean && npm run babel",
    "build:watch": "npm run clean && npm run babel -- --watch",
    "server": "cross-env PORT=3000 node dist/server",
    "devtools": "cross-env PORT=3001 webmiddle-devtools",
    "start": "nodemon --watch src --exec \"npm run build && npm run server\"",
    "start:devtools": "yarn run devtools & yarn run start",
    "prepublishOnly": "npm run build && npm run test"
  },
  "ava": {
    "files": [
      "src/tests/*.js",
      "!**/index.js"
    ],
    "sources": [
      "**/*.{js,jsx}",
      "!dist/**/*"
    ],
    "concurrency": 5,
    "failFast": true,
    "tap": true,
    "require": [
      "@babel/register"
    ],
    "babel": {
      "testOptions": {
        "extends": "./babel.config.js"
      }
    }
  },
  "dependencies": {
    "webmiddle": "^0.4.0",
    "webmiddle-component-cheerio-to-json": "^0.4.0",
    "webmiddle-component-http-request": "^0.4.0",
    "webmiddle-component-jsonselect-to-json": "^0.4.0",
    "webmiddle-component-parallel": "^0.4.0",
    "webmiddle-component-pipe": "^0.4.0",
    "webmiddle-component-resume": "^0.4.0",
    "webmiddle-devtools": "^0.2.1",
    "webmiddle-server": "^0.4.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.1.2",
    "@babel/core": "^7.0.0",
    "@babel/plugin-proposal-class-properties": "^7.0.0",
    "@babel/plugin-transform-react-jsx": "^7.0.0",
    "@babel/plugin-transform-runtime": "^7.1.0",
    "@babel/preset-env": "^7.1.0",
    "@babel/register": "^7.0.0",
    "@babel/runtime": "^7.1.2",
    "ava": "1.0.0-rc.1",
    "babel-eslint": "^10.0.1",
    "cross-env": "^5.2.0",
    "eslint": "^5.7.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-config-prettier": "^3.1.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jsx-a11y": "^6.1.2",
    "eslint-plugin-prettier": "^3.0.0",
    "eslint-plugin-react": "^v7.11.1",
    "husky": "^1.1.2",
    "lint-staged": "^7.3.0",
    "nodemon": "^1.18.4",
    "prettier": "^1.14.3",
    "rimraf": "^2.6.1"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.js": [
      "prettier --write",
      "git add"
    ]
  }
}
